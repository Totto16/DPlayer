        "yorkie": "^2.0.0"
    },
    "engines": {
        "node": "^v16.13.0"
    },
    "dependencies": {
        "axios": "^0.24.0",
        "balloon-css": "1.2.0",
        "rxjs": "^7.5.2"
    }
}
