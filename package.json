        "imagemin": "^8.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-svgo": "^10.0.1",
        "node-polyfill-webpack-plugin": "^1.1.4",
        "node-sass": "^7.0.1",
        "postcss-loader": "^6.2.1",
        "prettier": "^2.5.1",
        "prettier-check": "^2.0.0",
        "pretty-quick": "^3.1.3",
        "regenerator-runtime": "^0.13.9",
        "sass-loader": "^12.4.0",
