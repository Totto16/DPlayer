{
    "name": "dplayer",
    "version": "1.28.10-dev",
    "description": "Wow, such a lovely HTML5 danmaku video player",
    "main": "dist/DPlayer.min.js",
    "scripts": {
        "start": "npm run dev",
        "build": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress --color",
        "analyze": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress --color --analyze",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.config.js --color",
        "test": "eslint src webpack",
        "format": "eslint \"**/*.js\" --fix && prettier \"**/*.{js,json,md}\" --write",
        "format:staged": "eslint \"**/*.js\" --fix && pretty-quick --staged --verbose --pattern \"**/*.{js,json,md}\"  --ignore-path \"./.eslintignore\" ",
        "format:check": "eslint \"**/*.js\" && prettier-check \"**/*.{js,json,md}\"",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "check:update": "npm-check -u --skip-unused",
        "______comment": "npm-check needs to be installed globally: `npm i -g npm-check`"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "url": "git+https://github.com/Totto16/DPlayer.git",
        "type": "git"
    },
    "keywords": [
        "player",
        "danmaku",
        "video",
        "html5"
    ],
    "gitHooks": {
        "pre-commit": "npm run format:staged"
    },
    "author": "DIYgod",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DIYgod/DPlayer/issues"
    },
    "homepage": "https://github.com/DIYgod/DPlayer#readme",
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "@vuepress/plugin-back-to-top": "^1.8.2",
        "@vuepress/plugin-google-analytics": "^1.8.2",
        "@vuepress/plugin-pwa": "^1.8.2",
        "art-template": "^4.13.2",
        "art-template-loader": "^1.4.3",
        "autoprefixer": "^10.4.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.2.3",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "cross-env": "^7.0.0",
        "css-loader": "^6.5.1",
        "cssnano": "^5.0.11",
        "eslint": "^8.3.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "file-loader": "^6.0.0",
        "fontkit": "^1.8.1",
        "git-revision-webpack-plugin": "^5.0.0",
        "node-polyfill-webpack-plugin": "^1.1.4",
        "node-sass": "^6.0.1",
        "postcss-loader": "^6.2.0",
        "prettier": "^2.4.1",
        "prettier-check": "^2.0.0",
        "pretty-quick": "^3.1.2",
        "regenerator-runtime": "^0.13.9",
        "sass-loader": "^12.3.0",
        "simple-keyboard": "^3.3.27",
        "style-loader": "^3.3.1",
        "svg-inline-loader": "0.8.2",
        "template-string-optimize-loader": "^3.0.0",
        "url-loader": "^4.1.1",
        "vuepress": "^1.8.2",
        "webpack": "^5.64.2",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.5.0",
        "yorkie": "^2.0.0"
    },
    "dependencies": {
        "axios": "^0.24.0",
        "balloon-css": "1.2.0",
        "rxjs": "^7.4.0"
    }
}
