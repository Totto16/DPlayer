{
    "name": "dplayer",
    "version": "1.28.0-dev",
    "description": "Wow, such a lovely HTML5 danmaku video player",
    "main": "dist/DPlayer.min.js",
    "scripts": {
        "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --config webpack/prod.config.ts --progress --color",
        "analyze": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --config webpack/prod.config.ts --progress --color --analyze",
        "dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server --config webpack/dev.config.ts --color",
        "test": "eslint src webpack",
        "format": "eslint \"**/*.js\" --fix && prettier \"**/*.{js,json,md}\" --write",
        "format:staged": "",
        "format:staged_todo": "eslint \"**/*.js\" --fix && pretty-quick --staged --verbose --pattern \"**/*.{js,json,md}\"  --ignore-path \"./.eslintignore\" ",
        "format:check": "eslint \"**/*.js\" && prettier-check \"**/*.{js,json,md}\"",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "check:update": "npm-check -u --skip-unused",
        "______comment": "npm-check needs to be installed globally: `npm i -g npm-check`",
        "prettier:debug": "npx prettier --check .",
        "prettier": "npx prettier --write ."
    },
    "files": [
        "dist"
    ],
    "repository": {
        "url": "git+https://github.com/Totto16/DPlayer.git",
        "type": "git"
    },
    "keywords": [
        "player",
        "danmaku",
        "video",
        "html5"
    ],
    "gitHooks": {
        "pre-commit": "npm run format:staged && npm run prettier"
    },
    "authors": [
        "DIYgod",
        "Totto"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DIYgod/DPlayer/issues"
    },
    "homepage": "https://github.com/DIYgod/DPlayer#readme",
    "devDependencies": {
        "@babel/core": "^7.16.7",
        "@babel/preset-env": "^7.16.8",
        "@types/axios": "^0.14.0",
        "@types/cssnano": "^5.0.0",
        "@types/node": "^16.11.19",
        "@types/svgo": "^2.6.1",
        "@types/webpack": "^5.28.0",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@vuepress/plugin-back-to-top": "^1.9.6",
        "@vuepress/plugin-google-analytics": "^1.9.6",
        "@vuepress/plugin-pwa": "^1.9.6",
        "art-template": "^4.13.2",
        "art-template-loader": "^1.4.3",
        "autoprefixer": "^10.4.2",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.2.3",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "cross-env": "^7.0.0",
        "css-loader": "^6.5.1",
        "cssnano": "^5.0.15",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-boundaries": "^2.8.0",
        "eslint-plugin-github": "^4.3.5",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-optimize-regex": "^1.2.1",
